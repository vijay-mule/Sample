				<ee:set-payload><![CDATA[%dw 2.0
output application/json
---
payload]]></ee:set-payload>
			</ee:message>
		</ee:transform>
	</flow>
	<sub-flow name="sampleSub_Flow" doc:id="13a353be-58d9-4699-aaab-bcbbd176a38c" >
