		<set-payload value="Good evening" doc:name="Set Payload" doc:id="122570da-e679-4015-aca0-d377ce35c607" />
		<logger level="INFO" doc:name="Logger" doc:id="2bcc9a27-ace8-4518-8ca1-256ba380ad8b" message="Subflow completed"/>
	</sub-flow>
</mule>
