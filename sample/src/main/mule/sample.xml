		<logger level="INFO" doc:name="Logger" doc:id="de229e92-85ca-4889-92d9-f6e087f2711a" message="Main flow completed"/>
		<set-payload value="main flow payload 2" doc:name="Set Payload" doc:id="b4fde76e-f03e-40ad-a81b-0fa8754a9244" />
		<ee:transform doc:name="Transform Message" doc:id="c2f7fc69-a2c8-4407-8210-04722faa85d2">
